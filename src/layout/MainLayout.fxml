	orientation="VERTICAL" prefHeight="550.0" prefWidth="270.0"
	stylesheets="/layout/css/main_layout.css"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.MainController" >
	<items>
      <StackPane>
         <children>
      		<fx:include fx:id="wallpaperLayout" source="WallpaperLayout.fxml" />
      		<fx:include fx:id="smsLayout" source="SMSLayout.fxml" />
         </children>
      </StackPane>
		<fx:include source="KeyboardLayout.fxml" fx:id="keyboardLayout" />
	</items>
</SplitPane>
